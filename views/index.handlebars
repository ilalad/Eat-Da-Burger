<h1>Eat-Da-Burger!</h1>

<ul class="float-left">
    {{#each burgers}}
    {{#unless devoured}}
    {{> burgers/burger-block devoured=true}}
    {{/unless}}
    {{/each}}
</ul>

<ul class="float-right">
    {{#each burgers}}
    {{#if devoured}}
    {{> burgers/burger devoured devoured=false}}
    {{/if}}
    {{/each}}
</ul>